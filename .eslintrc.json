{
  "extends": [
    "airbnb-base",
    "prettier",
    "plugin:prettier/recommended",
    "plugin:pug"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true
      }
    ]
  },
  "plugins": [
    "prettier",
    "pug"
  ]
}