{
  "name": "echopig",
  "description": "personal podcast feed service",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon --inspect=0.0.0.0 --watch src src/server.js",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js  --watchAll --runInBand",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx src",
    "check-types": "tsc --noEmit",
    "prettier": "prettier --parser typescript --ignore-path .gitignore \"src/**/*.{js,json,ts,tsx}\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm-run-all --parallel check-format check-types lint"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.12.1",
    "@ladjs/bootstrap-social": "^7.0.2",
    "app-root-path": "^3.0.0",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.4.1",
    "cheerio": "^1.0.0-rc.3",
    "connect-mongo": "^3.2.0",
    "csurf": "^1.11.0",
    "email-templates": "^7.0.4",
    "express": "^4.17.1",
    "express-flash": "0.0.2",
    "express-session": "^1.17.0",
    "express-validator": "^6.4.0",
    "express-winston": "^4.0.3",
    "helmet": "^3.21.3",
    "jquery": "^3.4.1",
    "mailgun-js": "^0.22.0",
    "moment": "^2.24.0",
    "mongoose": "^5.9.5",
    "mongoose-autopopulate": "^0.9.1",
    "node-sass-middleware": "^0.11.0",
    "nodemailer": "^6.4.5",
    "nodemailer-mailgun-transport": "^1.4.0",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "podcast": "^1.1.0",
    "popper.js": "^1.16.1",
    "pug": "^2.0.4",
    "puppeteer": "^2.1.1",
    "redirect-chain": "^1.0.1",
    "searchitunes": "^2.4.0",
    "serve-favicon": "^2.5.0",
    "shortid": "^2.2.15",
    "simple-get": "^3.1.0",
    "sugar": "^2.0.6",
    "universal-analytics": "^0.4.20",
    "uuid": "^3.4.0",
    "validator": "^12.2.0",
    "vcards-js": "^2.10.0",
    "winston": "^3.2.0",
    "winston-transport": "^4.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.3",
    "@types/express-session": "^1.17.0",
    "@types/node": "^13.9.2",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "acorn": "^7.1.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-jest": "^3.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-security": "^1.4.0",
    "husky": "^4.2.3",
    "is-ci": "^2.0.0",
    "is-ci-cli": "^2.0.0",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "ts-node": "^8.7.0",
    "typescript": "^3.8.3"
  }
}
